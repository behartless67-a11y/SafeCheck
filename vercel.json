{
  "rewrites": [
    { "source": "/accountability", "destination": "/public/accountability.html" },
    { "source": "/admin", "destination": "/public/admin.html" },
    { "source": "/", "destination": "/public/index.html" },
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ],
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": "api/*.csv"
    }
  }
}
